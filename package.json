{
  "name": "fhevm-sdk-monorepo",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK for building confidential dApps with ease",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "install:all": "npm install",
    "build:sdk": "npm run build --workspace=packages/fhevm-sdk",
    "build:all": "npm run build --workspaces --if-present",
    "dev:environmental": "npm run dev --workspace=examples/environmental-voting",
    "dev:nextjs": "npm run dev --workspace=examples/nextjs-demo",
    "deploy:environmental": "npm run deploy --workspace=examples/environmental-voting",
    "test:sdk": "npm run test --workspace=packages/fhevm-sdk",
    "test:all": "npm run test --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present"
  },
  "keywords": [
    "fhevm",
    "zama",
    "encryption",
    "privacy",
    "sdk",
    "blockchain",
    "ethereum",
    "confidential-computing"
  ],
  "author": "FHEVM SDK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/fhevm-sdk.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "prettier": "^3.3.3"
  }
}
