{
  "name": "environmental-voting-example",
  "version": "1.0.0",
  "description": "Environmental Voting - Example dApp using @fhevm/sdk",
  "private": true,
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:gas": "REPORT_GAS=true hardhat test",
    "deploy": "hardhat run scripts/deploy.js --network sepolia",
    "deploy:local": "hardhat run scripts/deploy.js --network localhost",
    "verify": "hardhat verify --network sepolia",
    "interact": "hardhat run scripts/interact.js --network sepolia",
    "interact:sdk": "node scripts/interact-sdk.js",
    "clean": "hardhat clean",
    "node": "hardhat node"
  },
  "dependencies": {
    "@fhevm/sdk": "workspace:*",
    "@fhevm/solidity": "^0.5.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.7",
    "@nomicfoundation/hardhat-ethers": "^3.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.8",
    "chai": "^4.4.1",
    "hardhat": "^2.22.6",
    "hardhat-gas-reporter": "^2.2.0"
  }
}
