{
  "name": "@fhevm/sdk",
  "version": "1.0.0",
  "description": "Universal SDK for building confidential dApps with FHEVM - framework agnostic and developer friendly",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.js",
      "types": "./dist/vue.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/react.ts src/vue.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "fhevm",
    "zama",
    "encryption",
    "privacy",
    "sdk",
    "react",
    "vue",
    "blockchain",
    "ethereum",
    "web3",
    "confidential",
    "homomorphic-encryption"
  ],
  "author": "FHEVM SDK Team",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "fhevmjs": "^0.5.0",
    "ethers": "^6.13.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0",
    "eslint": "^8.57.0"
  }
}
